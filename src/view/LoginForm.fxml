<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.Cursor?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.PasswordField?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.text.Font?>

<AnchorPane fx:id="rootLoginForm" prefHeight="447.0" prefWidth="312.0" style="-fx-background-color: #ffffff;" xmlns="http://javafx.com/javafx/8.0.171" xmlns:fx="http://javafx.com/fxml/1" fx:controller="controller.LoginFormController">
   <children>
      <Label layoutX="63.0" layoutY="23.0" prefHeight="50.0" prefWidth="188.0" text="Login To To-Do List" textOverrun="CENTER_ELLIPSIS" underline="true">
         <font>
            <Font name="System Bold" size="20.0" />
         </font>
      </Label>
      <Label layoutX="22.0" layoutY="108.0" prefHeight="26.0" prefWidth="88.0" text="User Name" />
      <Label layoutX="22.0" layoutY="183.0" prefHeight="26.0" prefWidth="68.0" text="Password" />
      <TextField fx:id="txtUserName" layoutX="21.0" layoutY="141.0" prefHeight="25.0" prefWidth="271.0" />
      <PasswordField fx:id="txtPassword" layoutX="22.0" layoutY="224.0" onAction="#txtPasswordOnAction" prefHeight="25.0" prefWidth="271.0" />
      <Button layoutX="93.0" layoutY="293.0" mnemonicParsing="false" onAction="#btnLoginOnAction" prefHeight="25.0" prefWidth="127.0" style="-fx-background-color: #182C61;" text="Login" textFill="WHITE">
         <font>
            <Font name="System Bold" size="15.0" />
         </font>
         <cursor>
            <Cursor fx:constant="HAND" />
         </cursor>
      </Button>
      <Label layoutX="86.0" layoutY="354.0" onMouseClicked="#lblCreateAccountOnMouseClicked" text="create a new account" textFill="#0653ee" underline="true">
         <cursor>
            <Cursor fx:constant="HAND" />
         </cursor>
      </Label>
      <Label layoutX="90.0" layoutY="412.0" text="Designed By - Dulan" />
   </children>
</AnchorPane>
